<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @club.name %>
</p>

<p>
  <b>Description:</b>
  <%= @club.description %>
</p>

<p>
  <b>Contact:</b>
  <%= @club.contact %>
</p>

<p>
  <b>Url:</b>
  <%= @club.url %>
</p>


<% if(@comments!= nil) then @comments.each do |comment| %>
<tr>
    <td><%= comment.title %>, Rating = <%= comment.rating %> </td>
    <td><%= comment.body %></td>
</tr>
<% end %>
<% end %>

<%= link_to 'Back', clubs_path %>
